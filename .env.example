# .env.example - copy to .env and edit as needed
# Production-oriented default: DEMO_MODE=false -> enable true if you want offline/demo mode
DEMO_MODE=true
LOG_LEVEL=INFO
APP_HOST=0.0.0.0
APP_PORT=8080

# OpenSearch (optional, recommended for production)
OPENSEARCH_HOST=http://opensearch:9200
OPENSEARCH_INDEX=osint-iocs
OPENSEARCH_USER=
OPENSEARCH_PASS=

# Redis cache (optional, recommended)
REDIS_HOST=redis
REDIS_PORT=6379

# MaxMind GeoLite2 (optional, free registration required)
GEOIP_DB_PATH=/data/GeoLite2-City.mmdb

# Optional free API keys (leave blank to disable)
OTX_API_KEY= 
ABUSEIPDB_API_KEY=

# Scheduler
SCHEDULE_INTERVAL_MINUTES=15

# STIX settings
STIX_TLP=AMBER
STIX_AUTHOR=OSINT Pipeline

# Security
ALLOW_PUBLIC_FETCH=true
